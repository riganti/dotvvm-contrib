<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
	  <TargetFrameworks>$(ControlTargetFrameworks)</TargetFrameworks>
	  <PackageId>DotVVM.Contrib.HeroIcon</PackageId>
