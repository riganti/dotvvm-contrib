<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.1;net472</TargetFrameworks>
		<PackageId>DotVVM.Contrib.MultilevelMenu</PackageId>
		<Description>The MultilevelMenu control for DotVVM made by DotVVM Contrib community.</Description>
		<AssemblyName>DotVVM.Contrib.MultilevelMenu</AssemblyName>
	</PropertyGroup>

	<ItemGroup>
		<EmbeddedResource Include="Scripts\DotVVM.Contrib.MultilevelMenu.js" />
		<EmbeddedResource Include="Styles\DotVVM.Contrib.MultilevelMenu.css" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="DotVVM" Version="$(DotvvmVersion)" />
	</ItemGroup>

</Project>